@model IEnumerable<AppUser>

    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Update</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>
                    <a asp-action="Update" asp-route-id="@item.Id">Update</a>
                </td>
                <td>
                    <form asp-action="Delete" asp-route-id="@item.Id" method="post">
                        <button type="submit">Delete</button>
                    </form>
                </td>
            </tr>
            
        }
        </tbody>
    </table>